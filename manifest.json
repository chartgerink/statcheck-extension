{
	"manifest_version": 2,
	"name": "statcheck",
	"version": "0.0.9",
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Check stats?"
	},
	"content_scripts": [
	       {
		       "matches": ["<all_urls>"],
		       "js": ["script/mark.min.js",
			      "content.js"]
	       }
	]
}
